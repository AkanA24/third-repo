pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/AkanA24/third-repo.git'
            }
        }
        stage('Build') {
            steps {
                bat 'echo "Building..."'
            }
        }
        stage('Test') {
            steps {
                bat 'echo "Testing..."'
            }
        }
        stage('Deploy') {
            steps {
                  bat 'echo "deploying..."'
            }
        }
    }
}
